{
  "name": "Go Helper",

  "version": "0.5.6",

  "manifest_version": 3,
  "description": "",
  "permissions": ["activeTab", "scripting","tabs","storage","clipboardWrite","clipboardRead"],
  "icons": {
      "16": "icons/aa16.png",
      "32": "icons/aa48.png",
      "48": "icons/aa48.png",
      "128": "icons/aa128.png"
  },
  "action": {
      "default_popup": "popup.html",
      "default_icon": "icons/aa16.png",
      "default_title": "Go Helper"
  },
  "content_scripts": [
  {
    "matches": ["<all_urls>"],
    "js": ["content.js"],
    "run_at": "document_end"
  }
]
}
