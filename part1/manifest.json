{
  "name": "Agent Assist",

  "version": "0.4.0",

  "manifest_version": 3,
  "description": "",
  "permissions": ["activeTab", "scripting","tabs","storage","clipboardWrite","clipboardRead"],
  "icons": {
      "16": "icons/aa16.png",
      "32": "icons/aa48.png",
      "48": "icons/aa48.png",
      "128": "icons/aa128.png"
  },
  "action": {
      "default_popup": "popup/popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["popup/content.js"],
      "run_at": "document_end"
    }
  ]
}
