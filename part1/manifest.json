{
  "name": "2 minute timer",
  "version": "0.1.1",
  "manifest_version": 3,
  "description": "A 2 minute timer so you dont forget to get someone off hold.",
  "permissions": ["activeTab", "scripting","<all_urls>","tabs","storage","clipboardWrite","clipboardRead"],
  "icons": {
      "16": "icons/aa16.png",
      "32": "icons/aa48.png",
      "48": "icons/aa48.png",
      "128": "icons/aa128.png"
  },
  "action": {
      "default_popup": "popup/popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>","*://*.wikipedia.org/*"],
      "js": ["popup/content.js"],
      "run_at": "document_end"
    }
  ]
}
